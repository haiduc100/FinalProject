<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>home</title>
  </head>
  <body>
    <h1>body</h1>
    <table>
      <tr>
        <th>STT</th>
        <th>AssetName</th>
        <th>State</th>
        <th>Category</th>
        <th>AssetDate</th>
        <th>CreatedDate</th>
      </tr>
      <tbody>
        <% listAsset.map(function(asset, index){ %>
        <tr>
          <td><%= index+1 %></td>
          <td><%= asset.AssetName %></td>
          <td><%= asset.State %></td>
          <td><%= asset.Category.CategoryName %></td>
          <td><%= asset.AssetDate %></td>
          <td><%= asset.CreatedDate.toLocaleString() %></td>
        </tr>

        <%})%>
      </tbody>
    </table>
  </body>
</html>